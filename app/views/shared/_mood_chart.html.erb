<div class="chart-box">
  <h4>Mood history - last 14 days</h4>
  <%= line_chart chart_data,
      discrete: true,
      label: "Mood",
      ytitle: "Mood",
      loading: "Loading mood data...",
      empty: "No mood history",
      download: true,
      colors: ["#33372F", "#666"],
      legend: false %>
</div>
